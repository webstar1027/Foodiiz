{
  "name": "Foodiiz",
  "version": "1.0.0",
  "description": "This is the open git repository for Foodiiz collaborators.",
  "main": "index.php",
  "scripts": {
    "start": "npm run server",
    "server": "browser-sync start --server --files '*.html, dist/*.css, dist/*.js'",
    "clean": "rimraf dist/*",
    "build": "npm run clean && mkdir dist || npm-run-all --parallel build:*",
    "build:css": "npm-run-all --parallel build-sass:*",
    "build-sass": "node-sass assets/sass/index.scss > dist/foodiiz.css && cleancss -o dist/foodiiz.min.css dist/foodiiz.css && rimraf dist/foodiiz.css",
    "build:foodiiz": "browserify -t envify assets/js/app/app.module.js | uglifyjs -o dist/foodiiz.min.js",
    "watch:foodiiz": "watchify -t envify assets/js/app/app.module.js -d -o dist/foodiiz.min.js",
    "watch:css": "npm-run-all --parallel watch-sass:*",
    "watch-sass:foodiiz": "npm run watch-css:foodiiz && npm run watch-css:foodiiz -- -w",
    "watch-css:foodiiz": "node-sass --source-map dist/foodiiz.min.css.map assets/sass/index.scss dist/foodiiz.min.css --output-style compressed"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.0",
    "angular-bootstrap3-datepicker": "^0.4.0",
    "angular-dynamic-locale": "^0.1.35",
    "angular-file-upload": "^2.5.0",
    "angular-i18n": "^1.6.10",
    "angular-messages": "^1.6.1",
    "angular-resource": "^1.6.0",
    "angular-sanitize": "^1.4.8",
    "angular-stripe": "^5.0.0",
    "angular-translate": "^2.8.1",
    "angular-translate-handler-log": "^2.17.1",
    "angular-translate-loader-static-files": "^2.17.1",
    "angular-trix": "^1.0.2",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "^0.3.2",
    "angular-wizard": "^0.10.0",
    "angularjs-dropdown-multiselect": "^2.0.0-beta.10",
    "angularjs-toaster": "^2.1.0",
    "bootstrap": "^3.3.7",
    "bootstrap-slider": "^10.0.0",
    "bootstrap-vertical-tabs": "^1.2.2",
    "browserify": "^13.1.1",
    "clean-css": "^3.4.20",
    "crypto-js": "^3.1.9-1",
    "envify": "^3.4.1",
    "fs": "0.0.1-security",
    "fs-web": "^1.0.1",
    "googleapis": "^17.1.0",
    "jquery": "^3.1.1",
    "jquery-latitude-longitude-picker-gmaps": "^1.1.0",
    "jquery-touchswipe": "^1.6.18",
    "lodash": "^4.17.4",
    "modernizr": "^3.5.0",
    "moment": "^2.17.1",
    "multiple-date-picker": "^2.1.2",
    "node-sass": "^3.12.2",
    "npm-run-all": "^3.1.1",
    "responsive-bootstrap-tabs": "^1.0.0",
    "textangular": "^1.5.16",
    "uglify-js": "^2.7.4",
    "watchify": "^3.8.0"
  },
  "devDependencies": {
    "angular-translate": "^2.17.1",
    "browser-sync": "^2.17.6"
  },
  "homepage": ""
}
